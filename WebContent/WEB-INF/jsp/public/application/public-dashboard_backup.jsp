<%@ include file="/common/taglibs.jsp"%>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Vast Training Opportunities</title>
</head>
<body>

			<div class="container">
				<section class="row">
					<div class="col s12 m6 l6">	
						<div class="card horizontal">
	      					<div class="card-stacked">
								<div class="card-content purple lighten-5">
									<h3 class="header center-align">Not IITA Staff</h3>
									<p class="center-align">Apply for a training opportunity as non-IITA staff</p>
								</div>
								<div class="card-action">
									<div class="appbtn">
										<a class="waves-effect waves-light btn deep-purple darken-4 center-align" href="<s:url namespace="/" action="application/index" />">Apply</a>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col s12 m6 l6">	
						<div class="card horizontal">
	      					<div class="card-stacked">
								<div class="card-content purple lighten-5">
									<h3 class="header center-align">IITA Staff/Registered Applicant</h3>
									<p class="center-align">Login with your Credentials</p>
								</div>
								<div class="card-action">
									<div class="regbtn">
										<a class="waves-effect waves-light btn deep-purple darken-4 center-align" href="<s:url namespace="/" action="login" />">Login</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</section>
			</div>
			
			
</body>
</html>