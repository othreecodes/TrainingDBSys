		<action name="pending" class="cfoAction" method="viewpending">
			<result>/WEB-INF/jsp/cfo/list.jsp</result>
		</action>
		<action name="review" class="cfoAction" method="reviewTa">
			<result name="success">/WEB-INF/jsp/cfo/review-form.jsp</result>
			<result name="error">/WEB-INF/jsp/cfo/list.jsp</result>
			<result name="input">/WEB-INF/jsp/cfo/list.jsp</result>
		</action>
		<action name="approve" class="cfoAction" method="approve">
			<result name="success" type="redirect-action">index</result>
