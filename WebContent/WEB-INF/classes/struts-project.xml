		</action>
		
		<action name="upload-report">
			<result>/WEB-INF/jsp/upload-report.jsp</result>
		</action>
		
		<action name="external-training" class="externalTrainingAction">
			<result name="success">/WEB-INF/jsp/training/external-training.jsp</result>
		</action>  
