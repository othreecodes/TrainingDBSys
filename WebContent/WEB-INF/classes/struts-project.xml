<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <!-- User access to Training Unit -->
    <package namespace="/u" name="users-browsing" extends="default">
        <action name="index" class="dashboardActionUser">
            <result>/WEB-INF/jsp/index-user.jsp</result>
        </action>
    </package>

    <!-- CFO Package -->
    <package name="unit-staff" namespace="/staff" extends="default">
        <!-- Staff section -->
        <action name="index" class="staffDashboardAction" method="execute">
            <result>/WEB-INF/jsp/staff/list.jsp</result>
        </action>
        <action name="delete" class="staffDashboardAction" method="delete">
            <result>/WEB-INF/jsp/staff/list.jsp</result>
            <result name="success" type="redirect-action">index</result>
        </action>
        <action name="edit" class="staffDashboardAction" method="edit">
            <result>/WEB-INF/jsp/staff/apply.jsp</result>
            <result name="new" type="redirect-action">apply?id=${id}</result>
        </action>
        <action name="apply" class="staffDashboardAction" method="createNew">
            <result>/WEB-INF/jsp/staff/apply.jsp</result>
            <result name="success" type="redirect-action">view?id=${id}</result>
            <result name="new">/WEB-INF/jsp/staff/apply.jsp</result>
        </action>
        <action name="save" class="staffDashboardAction" method="save">
            <result>/WEB-INF/jsp/staff/apply.jsp</result>
            <result name="input">/WEB-INF/jsp/staff/apply.jsp</result>
            <result name="error">/WEB-INF/jsp/staff/apply.jsp</result>
            <result name="success" type="redirect-action">view?id=${id}</result>
            <result name="new" type="redirect-action">apply?id=${id}</result>
        </action>

        <action name="supervisor" class="staffDashboardAction" method="endorse">
            <result name="success" type="redirect-action">review</result>
            <result name="error">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="input">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="monitor" type="redirect-action">view?id=${sd.id}</result>
        </action>

        <action name="cdo" class="staffDashboardAction" method="approveCDOHead">
            <result name="success" type="redirect-action">review</result>
            <result name="error">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="input">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="monitor" type="redirect-action">view?id=${sd.id}</result>
        </action>
        <action name="hod" class="staffDashboardAction" method="approveHod">
            <result name="success" type="redirect-action">review</result>
            <result name="error">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="input">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="monitor" type="redirect-action">view?id=${sd.id}</result>
        </action>
        <action name="cmt" class="staffDashboardAction" method="approveCommittee">
            <result name="success" type="redirect-action">review</result>
            <result name="error">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="input">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="monitor" type="redirect-action">view?id=${sd.id}</result>
        </action>
        <action name="ddg" class="staffDashboardAction" method="approveDDG">
            <result name="success" type="redirect-action">review</result>
            <result name="error">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="input">/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="monitor" type="redirect-action">view?id=${sd.id}</result>
        </action>

        <action name="staffdevelopment-document-details" class="staffdevelopmentDocumentAction">
            <result type="redirect-action">appdetails?id=${entityDocument.entity.id}</result>
        </action>
        <action name="staffdevelopment-document" class="staffdevelopmentDocumentAction">
            <result type="redirect-action">edit?id=${entityDocument.entity.id}</result>
        </action>

        <!-- Staff Application reviewing process -->
        <action name="review" class="staffApprovalAction">
            <result>/WEB-INF/jsp/review/liststaff.jsp</result>
            <result name="single" type="redirect-action">review/show?id=${paged.results[0].id}</result>
            <result name="error" type="redirect-action">index</result>
            <result name="input" type="redirect-action">index</result>
        </action>
        <action name="review/show" class="viewStaffDevAction">
            <result>/WEB-INF/jsp/review/staffform.jsp</result>
            <result name="error" type="redirect-action">review</result>
            <result name="input" type="redirect-action">review</result>
        </action>

        <action name="readonly" class="viewStaffDevAction">
            <result>/WEB-INF/jsp/request/view-staffdev-details.jsp</result>
            <result name="error">/WEB-INF/jsp/generic-error.jsp</result>
        </action>
        <action name="view" class="dispatchStaffDevAction">
            <result name="editable" type="redirect-action">edit?id=${sd.id}</result>
            <result name="readonly" type="redirect-action">readonly?id=${sd.id}</result>
            <result name="finalized" type="redirect-action">readonly?id=${sd.id}</result>
            <result name="evaluation" type="redirect-action">evaluation/form?id=${sd.id}</result>
            <result name="ddg" type="redirect-action">review/show?id=${sd.id}</result>
            <result name="supervisor" type="redirect-action">review/show?id=${sd.id}</result>
            <result name="hod" type="redirect-action">review/show?id=${sd.id}</result>
            <result name="cmt" type="redirect-action">review/show?id=${sd.id}</result>
            <result name="cdo" type="redirect-action">review/show?id=${sd.id}</result>
            <result name="error">/WEB-INF/jsp/generic-error.jsp</result>
            <result>/WEB-INF/jsp/generic-error.jsp</result>
        </action>

        <action name="evaluation" class="staffEvaluationAction" method="save">
            <result>/WEB-INF/jsp/staff/evaluation.jsp</result>
            <result name="error">/WEB-INF/jsp/staff/evaluation.jsp</result>
            <result name="input">/WEB-INF/jsp/staff/evaluation.jsp</result>
            <result name="finalized" type="redirect-action">view?id=${sd.id}</result>
        </action>

        <action name="evaluation/form" class="staffEvaluationAction" method="show">
            <result name="success" type="redirect-action">view?id={sd.id}</result>
            <result name="input">/WEB-INF/jsp/staff/evaluation.jsp</result>
            <result name="error">/WEB-INF/jsp/staff/evaluation.jsp</result>
        </action>
    </package>

    <package namespace="/iya" name="agripreneur-browsing" extends="default">

        <action name="index" class="dashboardIyaAction">
            <result>/WEB-INF/jsp/iya/index.jsp</result>
        </action>

        <action name="save-announcement" class="iyaAnnouncementAction" method="saveAnnouncement">
            <result name="input">/WEB-INF/jsp/iya/announcement.jsp</result>
            <result name="success">/WEB-INF/jsp/iya/announcement_details.jsp</result>
        </action>
        <action name="announcement" class="iyaAnnouncementAction"
                method="input">
            <result name="input">/WEB-INF/jsp/iya/announcement.jsp</result>
            <result name="success" type="redirect-action">index</result>
        </action>
        <action name="announcements" class="iyaAnnouncementAction">
            <result name="success">/WEB-INF/jsp/iya/announcementslisting.jsp
            </result>
            <result name="error" type="redirect-action">index</result>
        </action>
        <action name="announcement/view" class="iyaAnnouncementAction"
                method="details">
            <result name="success">/WEB-INF/jsp/iya/announcement_details.jsp
            </result>
            <result name="error" type="redirect-action">index</result>
        </action>

        <action name="announcement/edit" class="iyaAnnouncementAction">
            <result name="success">/WEB-INF/jsp/iya/announcement_edit.jsp</result>
        </action>

        <action name="announcement/delete" class="iyaAnnouncementAction" method="removeAnnouncement">
            <result name="success" type="redirect-action">announcements</result>
            <result name="error">index</result>
        </action>


		<action name="save" class="cdoAnnouncementAction" method="save">
			<result name="success" type="redirect-action">profile?id=${announcement.id}</result>
			<result name="error">/WEB-INF/jsp/announcement/cdoannouncement-form.jsp</result>
			<result name="input">/WEB-INF/jsp/announcement/cdoannouncement-form.jsp</result>
		</action>
		
		<action name="profile" class="cdoAnnouncementAction">
			<result name="success">/WEB-INF/jsp/announcement/cdoannouncement-detail.jsp</result>
		</action>
		
		<action name="doupdate" class="cdoAnnouncementAction">
			<result name="success">/WEB-INF/jsp/announcement/cdoannouncement-form.jsp</result>
		</action>
		
		<action name="update" class="cdoAnnouncementAction" method="save">
			<result name="success" type="redirect-action">profile</result>
			<result name="error">/WEB-INF/jsp/announcement/cdoannouncement-form.jsp</result>
			<result name="input">/WEB-INF/jsp/announcement/cdoannouncement-form.jsp</result>
		</action>
				
		<action name="announcement-document" class="announcementDocumentAction">
			<result type="redirect-action">profile?id=${entityDocument.entity.id}</result>
		</action>
		
		<action name="tags" class="announcementTagBuilderAction">
			<result name="reload" type="redirect-action">announcement/profile?id=${profile.id}</result>
		</action>
	</package>
